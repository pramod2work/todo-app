{
  "name": "todo-app",
  "version": "1.0.0",
  "description": "Sample Todo app with skinned config for build and unit test",
  "main": "src/app/index.jsx",
  "comments": "dev build is diabled as browser sync is behaving funny, need to verify",
  "scripts": {
    "start": "cross-env BUILD_DOMAIN=devBuild npm-run-all dev",
    "prod": "npm-run-all -s clean:build build serve:build",
    "prod:build": "npm-run-all -s clean:build build",
    "build": "cross-env NODE_ENV=production webpack",
    "postinstall": "npm-run-all postinstall:*",
    "postinstall:husky": "node node_modules/husky/lib/installer/bin install",
    "precommit": "npm-run-all -p precommit:*",
    "prepush": "npm-run-all -p prepush:*",
    "clean:all": "npm-run-all -p clean:dll clean:build",
    "clean:build": "rimraf build",
    "clean:dll": "rimraf dll",
    "cz": "git-cz",
    "dev:build": "cross-env BUILD_DOMAIN=devBuild TEST_DOMAIN=ci npm-run-all prod",
    "precommit:verify": "npm run lint",
    "precommit:test": "npm run test",
    "precommit:build": "npm run build",
    "prepush:verify": "npm run lint",
    "prepush-disabled:test": "npm run test",
    "prepush-disabled:build": "npm run build",
    "dev": "cross-env NODE_ENV=development node ./config/browser-sync",
    "dll": "cross-env NODE_ENV=dll webpack",
    "format": "npm-run-all -p format:*",
    "format:scripts": "npm-run-all -p \"lint:scripts:* -- --fix\"",
    "format:styles": "stylefmt -r \"src/app/**/**/styles.js\"",
    "lint": "npm-run-all -p lint:*",
    "lint:styles": "stylelint \"src/app/**/*.js\"",
    "lint:scripts:standard": "standard \"src/app/**/*.js\" \"src/app/**/*.jsx\" \"config/**/*.js\"",
    "lint:scripts:eslint": "eslint \"src/app/**/*.js\" \"src/app/**/*.jsx\" \"config/**/*.js\"",
    "serve:build": "cross-env BUILD_DOMAIN=test TEST_DOMAIN=test  node ./test/serve-static.js",
    "storybook": "start-storybook -p 6007 -c ./config/storybook",
    "test": "cross-env NODE_ENV=unittest jest --config config/jest/config.json",
    "update:cache": "yarn cache clean && rimraf yarn.lock npm-packages-offline-cache node_modules && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pramod2work/react-skeleton.git"
  },
  "keywords": [
    "todo-app",
    "react bootstrap"
  ],
  "author": "pramod2work",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pramod2work/react-skeleton/issues"
  },
  "homepage": "https://github.com/pramod2work/react-skeleton#readme",
  "envVariableDescription": "NODE_ENV variable for webpack build and unit test, BUILD_DOMAIN is for build type that is going to point for sepcific domain id not specified then build is intended to production environemnt, TEST_DOMAIN is to specific test suite to pick required configuration",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@hot-loader/react-dom": "^16.8.4",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-hot-loader": "^4.8.0",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.4.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-knobs": "^5.0.1",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/cli": "^5.0.1",
    "@storybook/react": "^5.0.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-lodash": "^3.3.4",
    "browser-sync": "^2.26.3",
    "commitizen": "^3.0.7",
    "connect-history-api-fallback": "^1.6.0",
    "corp-semantic-release": "^6.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cz-customizable": "^5.5.3",
    "cz-customizable-ghooks": "^1.5.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.15.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.1",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^1.3.1",
    "ip": "^1.1.5",
    "jest": "^24.5.0",
    "jest-each": "^24.5.0",
    "jest-junit": "^6.3.0",
    "jest-sonar-reporter": "^2.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "stylefmt": "^6.0.3",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "stylelint-webpack-plugin": "^0.10.5",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-hot-middleware": "^2.24.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
